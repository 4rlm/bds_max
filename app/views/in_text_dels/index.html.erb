<p id="notice"><%= notice %></p>

<h1>In Text Dels</h1>

<table>
  <thead>
    <tr>
      <th>Term</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @in_text_dels.each do |in_text_del| %>
      <tr>
        <td><%= in_text_del.term %></td>
        <td><%= link_to 'Show', in_text_del %></td>
        <td><%= link_to 'Edit', edit_in_text_del_path(in_text_del) %></td>
        <td><%= link_to 'Destroy', in_text_del, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

