<p id="notice"><%= notice %></p>

<h2 class="page-header">Domainer</h2>

<div class="pull-right">
    <%= link_to 'Add Record', new_gcse_path, class: 'btn btn-primary' %>
    <%= link_to 'Import CSV', gcse_import_page_path, class: 'btn btn-primary' %>
    <%= link_to 'Download CSV', gcses_path(format: "csv"), class: 'btn btn-primary' %>
</div>

<%= form_tag "/gcses", method: :get, class: 'form-inline'  do %>
<table class="list-table table table-bordered table-hover text-nowrap table-striped">
    <thead>
        <!-- <tr id="exclude-col-select">
            <div class="form-group"> -->
                <%#= select_tag :columns, options_for_select(column_list, params[:columns]), title: 'Exclude Columns', multiple: true, class: 'selectpicker', title: "[]" %>
                <%= submit_tag "Submit", class: 'btn btn-success' %>
            <!-- </div>
        </tr> -->
        <tr>
            <th>
                <%= check_box_tag 'check_all', 'check_all', false, onClick: 'checkAll(this);' %>
            </th>

            <th>#</th>

            <!-- <% unless @col_domain_status %> -->
            <th>
                <div class="form-group">
                    <%= select_tag :domain_status, options_for_select(ordered_list(@selected_data.select(:domain_status).map(&:domain_status)), params[:domain_status]), multiple: true, class: 'selectpicker', title: "[]" %>
                    <br><br>Domain Status
                </div>
            </th>
            <!-- <% end %>

            <% unless @col_sfdc_root %> -->
            <th>
                <div class="form-group">

                    <%= text_field_tag :sfdc_root, params[:sfdc_root], placeholder: "----", class: 'form-control' %>
                    <!-- == Change to Keyword Search == -->

                    <%#= select_tag :sfdc_root, options_for_select(ordered_list(@selected_data.select(:sfdc_root).map(&:sfdc_root)), params[:sfdc_root]), multiple: true, class: 'selectpicker', title: "[]" %>
                    <br><br>SFDC Root
                </div>
            </th>
            <!-- <% end %>

            <% unless @col_root %> -->
            <th>
                <div class="form-group">

                    <%= text_field_tag :root, params[:root], placeholder: "----", class: 'form-control' %>
                    <!-- == Change to Keyword Search == -->

                    <%#= select_tag :root, options_for_select(ordered_list(@selected_data.select(:root).map(&:root)), params[:root]), multiple: true, class: 'selectpicker', title: "[]" %>
                    <br><br>New Root
                </div>
            </th>
            <!-- <% end %>

            <% unless @col_sfdc_acct %> -->
            <th>
                <div class="form-group">

                    <%= text_field_tag :sfdc_acct, params[:sfdc_acct], placeholder: "----", class: 'form-control' %>
                    <!-- == Change to Keyword Search == -->

                    <%#= select_tag :sfdc_acct, options_for_select(blank_remover(ordered_list(@selected_data.select(:sfdc_acct).map(&:sfdc_acct))), params[:sfdc_acct]), multiple: true, class: 'selectpicker', title: "[]" %>
                    <br><br>SFDC Acct
                </div>
            </th>
            <!-- <% end %>

            <% unless @col_sfdc_ult_acct %> -->
            <th>
                <div class="form-group">

                    <%= text_field_tag :sfdc_ult_acct, params[:sfdc_ult_acct], placeholder: "----", class: 'form-control' %>
                    <!-- == Change to Keyword Search == -->

                    <%#= select_tag :sfdc_ult_acct, options_for_select(blank_remover(ordered_list(@selected_data.select(:sfdc_ult_acct).map(&:sfdc_ult_acct))), params[:sfdc_ult_acct]), multiple: true, class: 'selectpicker', title: "[]" %>
                    <br><br>SFDC Ult Acct
                </div>
            </th>
            <!-- <% end %>

            <% unless @col_sfdc_url_o %> -->
            <th>
                <div class="form-group">

                    <%= text_field_tag :sfdc_url_o, params[:sfdc_url_o], placeholder: "----", class: 'form-control' %>
                    <!-- == Change to Keyword Search == -->

                    <%#= select_tag :sfdc_url_o, options_for_select(blank_remover(ordered_list(@selected_data.select(:sfdc_url_o).map(&:sfdc_url_o))), params[:sfdc_url_o]), multiple: true, class: 'selectpicker', title: "[]" %>
                    <br><br>SFDC Url
                </div>
            </th>
            <!-- <% end %>

            <% unless @col_domain %> -->
            <th>
                <div class="form-group">

                    <%= text_field_tag :domain, params[:domain], placeholder: "----", class: 'form-control' %>
                    <!-- == Change to Keyword Search == -->

                    <%#= select_tag :domain, options_for_select(ordered_list(@selected_data.select(:domain).map(&:domain)), params[:domain]), multiple: true, class: 'selectpicker', title: "[]" %>
                    <br><br>New Domain
                </div>
            </th>
            <!-- <% end %>

            <% unless @col_sfdc_street %> -->
            <th>
                <div class="form-group">
                    <%= text_field_tag :sfdc_street, params[:sfdc_street], placeholder: "----", class: 'form-control' %>
                    <br><br>SFDC Street
                </div>
            </th>
            <!-- <% end %>

            <% unless @col_sfdc_city %> -->
            <th>
                <div class="form-group">

                    <%= text_field_tag :sfdc_city, params[:sfdc_city], placeholder: "----", class: 'form-control' %>
                    <!-- == Change to Keyword Search == -->

                    <%#= select_tag :sfdc_city, options_for_select(ordered_list(@selected_data.select(:sfdc_city).map(&:sfdc_city)), params[:sfdc_city]), multiple: true, class: 'selectpicker', title: "[]" %>
                    <br><br>SFDC City
                </div>
            </th>
            <!-- <% end %>

            <% unless @col_sfdc_state %> -->
            <th>
                <div class="form-group">

                    <%= text_field_tag :sfdc_state, params[:sfdc_state], placeholder: "----", class: 'form-control' %>
                    <!-- == Change to Keyword Search == -->

                    <%#= select_tag :sfdc_state, options_for_select(ordered_list(@selected_data.select(:sfdc_state).map(&:sfdc_state)), params[:sfdc_state]), multiple: true, class: 'selectpicker', title: "[]" %>
