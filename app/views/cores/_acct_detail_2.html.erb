<table class="table table-bordered no-wrap">

    <tr>
        <%= acct_merge_sts_indicator(core) %>

        <td class="geo w-xsm"><%= core.match_score %></td>

        <!-- source -->
        <td class="geo w-xsm">CRM</td>

        <% if core.sfdc_clean_url %>
        <td>
            <%= link_to core.sfdc_clean_url, :target => "_blank", class: 'tooltips', title: "Raw URL: #{core.sfdc_url}\nClean URL: #{core.sfdc_clean_url}", data: { toggle: 'tooltip', placement: 'top'} do %>
            <%= core.sfdc_clean_url %>
            <% end %>
        </td>
        <% else %>
            <td>No SFDC Clean URL</td>
        <% end %>

        <!-- TRIAL SFDC Acct Name Map Module -->
        <% if core.sfdc_acct %>
            <td><span class="modalButton" data-toggle="modal" data-target="#previewModal" data-src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCMINjz6qATKxuxTc6tWNzyVehLybEYouY&zoom=17&q=<%= "#{core.sfdc_acct}" %>"><span data-toggle="tooltip" data-placement="top" title="Map: SFDC Name Only"><%= core.sfdc_acct %></span></span></td>
        <% else %>
            <td><span data-toggle="tooltip" data-placement="top" title="No SFDC Account Name">No Account Name</span></td>
        <% end %>

        <!-- CRM Addr Pin -->
        <% if core.crm_acct_pin %>
            <td class="geo w-md"><span data-toggle="tooltip" data-placement="top" title="SFDC Pin (Zip + Street #)"><%= core.crm_acct_pin %></span></td>
        <% else %>
            <td class="geo w-md"><span data-toggle="tooltip" data-placement="top" title="No SFDC Address Pin">No Pin</span></td>
        <% end %>

        <!-- TRIAL Map Module -->
        <% if core.full_address %>
            <td><span class="modalButton" data-toggle="modal" data-target="#previewModal" data-src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCMINjz6qATKxuxTc6tWNzyVehLybEYouY&zoom=17&q=<%= "#{core.full_address},#{core.sfdc_acct}" %>"><span data-toggle="tooltip" data-placement="top" title="Map: SFDC Address & Name"><%= core.full_address %></span></span></td>
        <% else %>
            <td><span data-toggle="tooltip" data-placement="top" title="No SFDC Address">No Address</span></td>
        <% end %>

        <% if core.sfdc_ph %>
            <td class="geo w-md"><span data-toggle="tooltip" data-placement="top" title="SFDC Account Phone"><%= core.sfdc_ph %></span></td>
        <% else %>
            <td class="geo w-md"><span data-toggle="tooltip" data-placement="top" title="No SFDC Phone">No Phone</span></td>
        <% end %>

        <!-- franchise brand type & Franchise Category Icon-->
        <% if core.sfdc_franch_cons && core.sfdc_franch_cat != nil %>
            <td class="geo w-xsm fa-blue"><%= fa_icon "car lg", "data-toggle": "tooltip", "data-placement": "top", title: "SDF #{core.sfdc_franch_cat}: #{core.sfdc_franch_cons}" %></td>
        <% else %>
            <td class="geo w-xsm fa-gray"><%= fa_icon "car lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Franchise Data" %></td>
        <% end %>
    </tr>

    <!-- Updated/Confirmed Account Details (name, address, url, franchise, etc.) -->
    <tr>
        <%= acct_merge_sts_indicator(core) %>

        <% if advanced_and_up %>
            <td class="geo w-xsm" data-id="<%= core.id %>" onClick="changeStatusCore(this);">
                <%= fa_icon "check lg", class: "stat-btn fa-clear", "data-toggle": "tooltip", "data-placement": "top", title: "Select" %>
            </td>
        <% else %>
            <td class="geo w-xsm">None</td>
        <% end %>


        <!-- Trial for Scraped Data Modal -->
        <td class="geo w-xsm"><span class="modalButton" data-toggle="modal" data-target="#scrapedModal" data-src="http://localhost:3000<%= indexer_show_detail_path(core: core) %>">Scraped</span></td>


        <% if core.alt_url %>
        <td>
            <%= link_to core.alt_url, :target => "_blank", class: 'tooltips', title: "Scraped Clean URL: #{core.alt_url}", data: { toggle: 'tooltip', placement: 'top'} do %>
            <%= core.alt_url %>
            <% end %>
        </td>
        <% else %>
            <td>No Scraped URL</td>
        <% end %>



        <!-- TRIAL SCRAPED Acct Name Map Module -->
        <% if core.alt_acct %>
            <td><span class="modalButton" data-toggle="modal" data-target="#previewModal" data-src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCMINjz6qATKxuxTc6tWNzyVehLybEYouY&zoom=17&q=<%= "#{core.alt_acct}" %>"><span data-toggle="tooltip" data-placement="top" title="Map: Scraped Name Only"><%= core.alt_acct %></span></span></td>
        <% else %>
            <td><span data-toggle="tooltip" data-placement="top" title="No Scraped Account Name">No Account Name</span></td>
        <% end %>



        <!-- Alt Addr Pin -->
        <% if core.alt_acct_pin %>
            <td class="geo w-md"><span data-toggle="tooltip" data-placement="top" title="Scraped Pin (Zip + Street #)"><%= core.alt_acct_pin %></span></td>
        <% else %>
            <td class="geo w-md"><span data-toggle="tooltip" data-placement="top" title="No Scraped Address Pin">No Pin</span></td>
        <% end %>


        <!-- TRIAL Map Module -->
        <% if core.alt_address %>
            <td><span class="modalButton" data-toggle="modal" data-target="#previewModal" data-src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCMINjz6qATKxuxTc6tWNzyVehLybEYouY&zoom=17&q=<%= "#{core.alt_address}, #{core.alt_acct}" %>"><span data-toggle="tooltip" data-placement="top" title="Map: Scraped Address & Name"><%= core.alt_address %></span></span></td>
        <% else %>
            <td><span data-toggle="tooltip" data-placement="top" title="No Address Scraped">No Address</span></td>
        <% end %>


        <% if core.alt_ph %>
            <td class="geo w-md"><span data-toggle="tooltip" data-placement="top" title="Scraped Phone"><%= core.alt_ph %></span></td>
        <% else %>
            <td class="geo w-md"><span data-toggle="tooltip" data-placement="top" title="No Scraped Phone Linked">No Phone</span></td>
        <% end %>


        <!-- franchise brand type & Franchise Category Icon-->
        <% if core.sfdc_franch_cons && core.sfdc_franch_cat != nil %>
            <td class="geo w-xsm fa-blue"><%= fa_icon "car lg", "data-toggle": "tooltip", "data-placement": "top", title: "SDF #{core.sfdc_franch_cat}: #{core.sfdc_franch_cons}" %></td>
        <% else %>
            <td class="geo w-xsm fa-gray"><%= fa_icon "car lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Franchise Data" %></td>
        <% end %>

    </tr>

</table>



<!-- This modal is only for the "Scraped Data" -->
<div class="modal fade" id="scrapedModal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-body">
                <div class="close-button">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                </div>
                <div class="embed-responsive embed-responsive-16by9">
                    <iframe class="embed-responsive-item" frameborder="2"></iframe>
                </div>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->
