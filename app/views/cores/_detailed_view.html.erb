<!-- theme-layout-table -->
<!-- profile-container -->
<!-- ### New Version from Geo Locations  ### -->
<% num = 0 %>
<% @cores.each do |core| %>
    <% num +=1 %>
    <div id="core-detail" class="text-nowrap geo-result-table">
        <div class="row">
            <div class="col-md-10"> <!-- Status Icons -->
                <%= render partial: 'cores/acct_detail_1', locals: { num: num, core: core } %>
                <%= render partial: 'cores/acct_detail_2', locals: { core: core } %>
                <%= render partial: 'cores/acct_detail_3', locals: { core: core } %>
            </div>
            <div class="col-md-2">
                <% if core.img_url %>
                    <td>
                        <%= link_to "https://www.google.com/maps/embed/v1/place?key=AIzaSyCMINjz6qATKxuxTc6tWNzyVehLybEYouY&zoom=17&q=#{core.sfdc_acct}", target: "_blank"  do %>
                            <%= image_tag(core.img_url, :class=>'geo-img', alt: core.geo_acct_name) %>
                        <% end %>
                    </td>
                <% end %>
            </div>
        </div>
    </div><br> <!-- Account Profile Ends -->
<% end %>

<!-- ### MODAL TEMPLATE!! - DO NOT DELETE   ### -->
<div class="modal fade" id="myModal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-body">
                <div class="close-button">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                </div>
                <div class="embed-responsive embed-responsive-16by9">
                    <iframe class="embed-responsive-item" frameborder="0"></iframe>
                </div>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->
