<!-- ### New Version from Geo Locations  ### -->
<!-- ################################ -->

<div id="core-detail" class="text-nowrap">
    <div class="row">

        <div class="col-md-8"> <!-- Status Icons -->
            <table class="table table-bordered geo-result-table">
                <tr>
                    <!-- Checkbox / Num -->
                    <!-- Dealer Website URL -->
                    <th><%= num %></th>
                    <!-- account id / sfdc acct link -->
                    <th>ID</th>
                    <!-- bds_status -->
                    <th>Core</th>
                    <!-- domainer status -->
                    <th>Dom</th>
                    <!-- indexer status -->
                    <th colspan="2">Indexer</th>
                    <!-- staffer status -->
                    <th colspan="2">Staffer</th>
                    <!-- dealer website template -->
                    <th>Temp</th>
                    <!-- geo location status -->
                    <th>Geo</th>
                    <!-- CoPilot Franchise and Type -->
                    <th>CoP</th>


                </tr>
                <tr>
                    <!-- tier -->
                    <td class="geo w-sm"><%= core.sfdc_tier %></td>

                    <!-- SFDC Account Icon -->
                    <% if core.sfdc_acct %>
                        <td class="geo w-xsm fa-blue"><%= fa_icon "address-card lg", "data-toggle": "tooltip", "data-placement": "top", title: "Account Name / ID: #{core.sfdc_acct}" %></td>
                    <% else %>
                        <td class="geo w-xsm fa-gray">
                            <%= fa_icon "address-card lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Account" %>
                        </td>
                    <% end %>






                    <!-- bds_status -->
                    <% unless core.updated_at == nil %>
                        <td class="geo w-xsm fa-blue">
                            <%= fa_icon "diamond lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.bds_status}: #{core.updated_at.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% else %>
                        <td class="geo w-xsm fa-gray">
                            <%= fa_icon "diamond lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Status" %>
                        </td>
                    <% end %>

                    <!-- domainer status -->
                    <% unless core.domain_status == nil %>
                        <td class="geo w-xsm fa-blue">
                            <%= fa_icon "globe lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.domain_status}: #{core.domainer_date.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% else %>
                        <td class="geo w-xsm fa-gray">
                            <%= fa_icon "globe lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Status" %>
                        </td>
                    <% end %>

                    <!-- staff indexer status -->
                    <% if core.indexer_date != nil && core.staff_indexer_status == "Matched" %>
                        <td class="geo w-xsm fa-blue">
                            <%= link_to core.staff_link, :target => "_blank" do %>
                            <%= fa_icon "users lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.staff_text}: #{core.indexer_date.strftime("%m/%d/%Y")}" %>
                            <% end %>
                        </td>
                    <% elsif core.indexer_date != nil && core.staff_indexer_status == "Indexer Result" %>
                        <td class="geo w-xsm fa-green">
                            <%= fa_icon "users lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.staff_indexer_status}: #{core.indexer_date.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% elsif core.indexer_date != nil && core.staff_indexer_status == "No Matches" %>
                        <td class="geo w-xsm fa-black">
                            <%= fa_icon "users lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.staff_indexer_status}: #{core.indexer_date.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% elsif core.indexer_date != nil && core.staff_indexer_status == "Delete" %>
                        <td class="geo w-xsm fa-red">
                            <%= fa_icon "warning lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.staff_indexer_status}: #{core.indexer_date.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% elsif core.indexer_date != nil && core.staff_indexer_status == "Flag" %>
                        <td class="geo w-xsm fa-red">
                            <%= fa_icon "flag lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.staff_indexer_status}: #{core.indexer_date.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% elsif core.indexer_date != nil && core.staff_indexer_status == "Queue Indexer" %>
                        <td class="geo w-xsm fa-orange">
                            <%= fa_icon "spinner pulse lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.staff_indexer_status}: #{core.indexer_date.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% else %>
                        <td class="geo w-xsm fa-gray">
                            <%= fa_icon "users lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Status" %>
                        </td>
                    <% end %>

                    <!-- loc indexer status -->
                    <% if core.indexer_date != nil && core.location_indexer_status == "Matched" %>
                        <td class="geo w-xsm fa-blue">
                            <%= link_to core.location_link, :target => "_blank" do %>
                            <%= fa_icon "sitemap lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.location_text}: #{core.indexer_date.strftime("%m/%d/%Y")}" %>
                            <% end %>
                        </td>
                    <% elsif core.indexer_date != nil && core.location_indexer_status == "Indexer Result" %>
                        <td class="geo w-xsm fa-green">
                            <%= fa_icon "sitemap lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.location_indexer_status}: #{core.indexer_date.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% elsif core.indexer_date != nil && core.location_indexer_status == "No Matches" %>
                        <td class="geo w-xsm fa-black">
                            <%= fa_icon "sitemap lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.location_indexer_status}: #{core.indexer_date.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% elsif core.indexer_date != nil && core.location_indexer_status == "Delete" %>
                        <td class="geo w-xsm fa-red">
                            <%= fa_icon "warning lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.location_indexer_status}: #{core.indexer_date.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% elsif core.indexer_date != nil && core.location_indexer_status == "Flag" %>
                        <td class="geo w-xsm fa-red">
                            <%= fa_icon "flag lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.location_indexer_status}: #{core.indexer_date.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% elsif core.indexer_date != nil && core.location_indexer_status == "Queue Indexer" %>
                        <td class="geo w-xsm fa-orange">
                            <%= fa_icon "spinner pulse lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.location_indexer_status}: #{core.indexer_date.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% else %>
                        <td class="geo w-xsm fa-gray">
                            <%= fa_icon "sitemap lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Status" %>
                        </td>
                    <% end %>

                    <!-- crm staffer status -->
                    <% if core.staffer_date != nil && core.staffer_status == "CRM Contacts"%>
                        <td class="geo w-xsm fa-blue">
                            <%= link_to staffer_acct_contacts_path(core: core), :target => "_blank" do %>
                                <%= fa_icon "user-plus lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.staffer_status}: #{core.staffer_date.strftime("%m/%d/%Y")}" %>
                            <% end %>
                        </td>
                    <% else %>
                        <td class="geo w-xsm fa-gray">
                            <%= fa_icon "user-plus lg", "data-toggle": "tooltip", "data-placement": "top", title: "No CRM Contacts" %>
                        </td>
                    <% end %>

                    <!-- web staffer status -->
                    <% if core.staffer_date != nil && core.staffer_status == "Web Contacts"%>
                        <td class="geo w-xsm fa-blue">
                            <%= link_to staffer_acct_contacts_path(core: core), :target => "_blank" do %>
                                <%= fa_icon "user-plus lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.staffer_status}: #{core.staffer_date.strftime("%m/%d/%Y")}" %>
                            <% end %>
                        </td>
                    <% elsif core.staffer_date != nil && core.staffer_status.include?("Error") %>
                        <td class="geo w-xsm fa-red">
                            <%= fa_icon "user-plus lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.staffer_status}: #{core.staffer_date.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% else %>
                        <td class="geo w-xsm fa-gray">
                            <%= fa_icon "user-plus lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Web Contacts" %>
                        </td>
                    <% end %>

                    <!-- dealer website template -->
                    <% if core.sfdc_template != nil && core.staffer_date != nil && (core.sfdc_template.include?("Dealer.com") || core.sfdc_template.include?("Dealer.com") || core.sfdc_template.include?("DealerOn") || core.sfdc_template.include?("Cobalt") || core.sfdc_template.include?("DealerFire") || core.sfdc_template.include?("DealerInspire") )%>
                        <td class="geo w-xsm fa-blue">
                            <%= fa_icon "file-text lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.sfdc_template}:  #{core.staffer_date.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% elsif core.sfdc_template != nil && core.staffer_date != nil && core.sfdc_template.include?("Error") %>
                        <td class="geo w-xsm fa-red">
                            <%= fa_icon "file-text lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.sfdc_template}:  #{core.staffer_date.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% else %>
                        <td class="geo w-xsm fa-gray">
                            <%= fa_icon "file-text lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Status" %>
                        </td>
                    <% end %>

                    <!-- geo location status -->
                    <% if core.geo_date != nil && core.geo_status == "Geo Result" %>
                        <td class="geo w-xsm fa-blue">
                            <%= fa_icon "map lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.geo_status}:  #{core.geo_date.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% elsif core.geo_date != nil && core.geo_status.include?("Error") %>
                        <td class="geo w-xsm fa-red">
                            <%= fa_icon "map lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.geo_status}:  #{core.geo_date.strftime("%m/%d/%Y")}" %>
                        </td>
                    <% else %>
                        <td class="geo w-xsm fa-gray">
                            <%= fa_icon "map lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Status" %>
                        </td>
                    <% end %>

                    <!-- CoPilot franchise brand type & Franchise Category Icon-->
                    <% if core.sfdc_franch_cons && core.sfdc_franch_cat != nil %>
                        <td class="geo w-xsm fa-blue"><%= fa_icon "car lg", "data-toggle": "tooltip", "data-placement": "top", title: "SDF #{core.sfdc_franch_cat}: #{core.sfdc_franch_cons}" %></td>
                    <% else %>
                        <td class="geo w-xsm fa-gray"><%= fa_icon "car lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Franchise Data" %></td>
                    <% end %>

                </tr>
            </table>

            <table class="table table-bordered geo-result-table"> <!-- account details -->


                <!-- Original Account Details (name, address, url, franchise, etc.) -->
                <tr>

                    <!-- Map Module -->
                    <% if core.sfdc_acct %>
                        <td class="geo w-xsm fa-blue">
                            <span class="modalButton" data-toggle="modal" data-target="#myModal" data-src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCMINjz6qATKxuxTc6tWNzyVehLybEYouY&zoom=17&q=<%= "#{core.sfdc_acct}" %>"><%= fa_icon "map-marker lg", "data-toggle": "tooltip", "data-placement": "top", title: "Map LightBox  #{core.coordinates}" %></span>
                        </td>
                    <% else %>
                        <td class="geo w-xsm fa-gray">
                            <%= fa_icon "map-marker lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Account" %>
                        </td>
                    <% end %>

                    <!-- Dealer Website Module -->
                    <% if core.sfdc_acct != nil && core.sfdc_url != nil %>
                        <td class="geo w-xsm fa-blue">
                        <span class="modalButton" data-toggle="modal" data-target="#myModal" data-src="<%= add_http(core.sfdc_url) %>">
                        <%= fa_icon "clone lg", "data-toggle": "tooltip", "data-placement": "top", title: "LightBox: Dealer Site" %></span></span>
                        </td>
                    <% elsif core.sfdc_acct != nil && core.sfdc_url == nil %>
                        <td class="geo w-xsm fa-gray"><%= fa_icon "clone lg", "data-toggle": "tooltip", "data-placement": "top", title: "No URL - Permanently Closed?" %></td>
                    <% else %>
                        <td class="geo w-xsm fa-red"><%= fa_icon "clone lg", "data-toggle": "tooltip", "data-placement": "top", title: "No URL" %></td>
                    <% end %>

                    <!-- Dealer Website External Link -->
                    <% unless core.sfdc_url == nil %>
                        <td class="geo w-xsm fa-blue">
                            <%= link_to add_http(core.sfdc_url), :target => "_blank" do %>
                                <%= fa_icon "external-link lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.sfdc_url}" %>
                            <% end %>
                        </td>
                    <% else %>
                        <td class="geo w-xsm fa-red"><%= fa_icon "external-link lg", "data-toggle": "tooltip", "data-placement": "top", title: "No URL - Permanently Closed?" %></td>
                    <% end %>

                    <!-- source -->
                    <td><%= core.acct_source %></td>

                    <!-- SFDC Acct Name -->
                    <td><%= core.sfdc_acct %></td>

                    <!-- SFDC core.full_address -->
                    <% if core.full_address %>
                        <td><%= link_to core.full_address, core, target: "_blank" %></td>
                    <% else %>
                        <td>No Address</td>
                    <% end %>

                    <!-- phone -->
                    <% unless core.sfdc_ph == nil %>
                        <td class="geo w-xsm fa-blue">
                            <%= fa_icon "phone lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.sfdc_ph}" %>
                        </td>
                    <% else %>
                        <td class="geo w-xsm fa-gray">
                            <%= fa_icon "phone lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Phone" %>
                        </td>
                    <% end %>

                    <!-- franchise brand type & Franchise Category Icon-->
                    <% if core.sfdc_franch_cons && core.sfdc_franch_cat != nil %>
                        <td class="geo w-xsm fa-blue"><%= fa_icon "car lg", "data-toggle": "tooltip", "data-placement": "top", title: "SDF #{core.sfdc_franch_cat}: #{core.sfdc_franch_cons}" %></td>
                    <% else %>
                        <td class="geo w-xsm fa-gray"><%= fa_icon "car lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Franchise Data" %></td>
                    <% end %>

                </tr>





                <!-- Updated/Confirmed Account Details (name, address, url, franchise, etc.) -->
                <tr>
                    <!-- Map Module -->
                    <% if core.sfdc_acct %>
                        <td class="geo w-xsm fa-blue">
                            <span class="modalButton" data-toggle="modal" data-target="#myModal" data-src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCMINjz6qATKxuxTc6tWNzyVehLybEYouY&zoom=17&q=<%= "#{core.sfdc_acct}" %>"><%= fa_icon "map-marker lg", "data-toggle": "tooltip", "data-placement": "top", title: "Map LightBox  #{core.coordinates}" %></span>
                        </td>
                    <% else %>
                        <td class="geo w-xsm fa-gray">
                            <%= fa_icon "map-marker lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Account" %>
                        </td>
                    <% end %>

                    <!-- Dealer Website Module -->
                    <% if core.sfdc_acct != nil && core.sfdc_url != nil %>
                        <td class="geo w-xsm fa-blue">
                        <span class="modalButton" data-toggle="modal" data-target="#myModal" data-src="<%= add_http(core.sfdc_url) %>">
                        <%= fa_icon "clone lg", "data-toggle": "tooltip", "data-placement": "top", title: "LightBox: Dealer Site" %></span></span>
                        </td>
                    <% elsif core.sfdc_acct != nil && core.sfdc_url == nil %>
                        <td class="geo w-xsm fa-gray"><%= fa_icon "clone lg", "data-toggle": "tooltip", "data-placement": "top", title: "No URL - Permanently Closed?" %></td>
                    <% else %>
                        <td class="geo w-xsm fa-red"><%= fa_icon "clone lg", "data-toggle": "tooltip", "data-placement": "top", title: "No URL" %></td>
                    <% end %>

                    <!-- Dealer Website External Link -->
                    <% unless core.sfdc_url == nil %>
                        <td class="geo w-xsm fa-blue">
                            <%= link_to add_http(core.sfdc_url), :target => "_blank" do %>
                                <%= fa_icon "external-link lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.sfdc_url}" %>
                            <% end %>
                        </td>
                    <% else %>
                        <td class="geo w-xsm fa-red"><%= fa_icon "external-link lg", "data-toggle": "tooltip", "data-placement": "top", title: "No URL - Permanently Closed?" %></td>
                    <% end %>

                    <!-- source -->
                    <td><%= core.acct_source %></td>

                    <!-- SFDC Acct Name -->
                    <td><%= core.sfdc_acct %></td>

                    <!-- SFDC core.full_address -->
                    <% if core.full_address %>
                        <td><%= link_to core.full_address, core, target: "_blank" %></td>
                    <% else %>
                        <td>No Address</td>
                    <% end %>

                    <!-- phone -->
                    <% unless core.sfdc_ph == nil %>
                        <td class="geo w-xsm fa-blue">
                            <%= fa_icon "phone lg", "data-toggle": "tooltip", "data-placement": "top", title: "#{core.sfdc_ph}" %>
                        </td>
                    <% else %>
                        <td class="geo w-xsm fa-gray">
                            <%= fa_icon "phone lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Phone" %>
                        </td>
                    <% end %>

                    <!-- franchise brand type & Franchise Category Icon-->
                    <% if core.sfdc_franch_cons && core.sfdc_franch_cat != nil %>
                        <td class="geo w-xsm fa-blue"><%= fa_icon "car lg", "data-toggle": "tooltip", "data-placement": "top", title: "SDF #{core.sfdc_franch_cat}: #{core.sfdc_franch_cons}" %></td>
                    <% else %>
                        <td class="geo w-xsm fa-gray"><%= fa_icon "car lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Franchise Data" %></td>
                    <% end %>

                </tr>


            </table>
        </div>



        <div class="col-md-4">  <!-- group account data info -->



            <table class="table table-bordered geo-result-table">
                <tr>
                    <!-- type -->
                    <th><%= fa_icon "tags lg", "data-toggle": "tooltip", "data-placement": "top", title: "Account Type" %></th>
                    <!-- sales_person -->
                    <th><%= fa_icon "user-circle lg", "data-toggle": "tooltip", "data-placement": "top", title: "Salesperson" %></th>
                </tr>
                <tr>
                    <!-- type -->
                    <td><%= core.sfdc_type %></td>
                    <!-- sales_person -->
                    <td><%= core.sfdc_sales_person %></td>
                </tr>
            </table>




            <table class="table table-bordered geo-result-table">
                <tr>
                    <!-- group name -->
                    <% if core.sfdc_group %>
                        <td class="geo w-xsm fa-blue"><%= fa_icon "cubes lg", "data-toggle": "tooltip", "data-placement": "top", title: "Group Name" %></td>
                    <% else %>
                        <td class="geo w-xsm fa-gray"><%= fa_icon "cubes lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Group Name" %></td>
                    <% end %>

                    <!-- group name -->
                    <% if core.sfdc_group %>
                        <td class="geo w-xsm"><%= core.sfdc_grp_rt %></td>
                        <td><%= link_to core.sfdc_group, core, target: "_blank" %></td>
                    <% else %>
                        <td>No Group</td>
                    <% end %>

                    <!-- Ultimate Group: core.sfdc_ult_grp -->
                    <% if core.sfdc_ult_grp %>
                        <td class="geo w-xsm fa-blue"><%= fa_icon "cube lg", "data-toggle": "tooltip", "data-placement": "top", title: "Ultimate Group Account" %></td>
                    <% else %>
                        <td class="geo w-xsm fa-gray"><%= fa_icon "cube lg", "data-toggle": "tooltip", "data-placement": "top", title: "No Ultimate Group Account" %></td>
                    <% end %>

                    <!-- ultimate group name -->
                    <% if core.sfdc_ult_grp %>
                        <!-- ultimate group rt -->
                        <td class="geo w-xsm"><%= core.sfdc_ult_rt %></td>
                        <td><%= link_to core.sfdc_ult_grp, core, target: "_blank" %></td>
                    <% else %>
                        <td>No Ultimate Group</td>
                    <% end %>

                </tr>
            </table>

        </div>
    </div>


</div>  <!-- Account Profile Ends -->

