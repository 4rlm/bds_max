<table class="table table-bordered geo-result-table">
    <thead>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Work Phone</th>
            <th>Mobile Phone</th>
            <th>Department</th>
            <th colspan="2">Manage</th>
        </tr>
    </thead>
    <tbody>
        <% users.each do |user| %>
            <tr>
                <td><%= user.id %></td>
                <td><%= user.first_name %></td>
                <td><%= user.last_name %></td>
                <td><%= user.email %></td>
                <td><%= user.work_phone %></td>
                <td><%= user.mobile_phone %></td>
                <td><%= user.department %></td>
                <td class='edit-btn' >
                    <%#= link_to edit_user_registration_path, method: :edit do %>
                        <span class="glyphicon glyphicon-pencil"></span>
                    <%# end %>
                </td>
                <td class='delete-btn' >
                    <%#= link_to core, method: :delete do %>
                        <span class="glyphicon glyphicon-trash"></span>
                    <%# end %>
                </td>
            </tr>
        <% end %>
    </tbody>
</table>
