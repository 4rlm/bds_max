<p id="notice"><%= notice %></p>

<h1>In Host Pos</h1>

<table>
  <thead>
    <tr>
      <th>Term</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @in_host_pos.each do |in_host_po| %>
      <tr>
        <td><%= in_host_po.term %></td>
        <td><%= link_to 'Show', in_host_po %></td>
        <td><%= link_to 'Edit', edit_in_host_po_path(in_host_po) %></td>
        <td><%= link_to 'Destroy', in_host_po, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

